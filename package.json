  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/nutes-uepb/ip-allowed/issues"
  },
  "homepage": "https://github.com/nutes-uepb/ip-allowed#readme",
  "dependencies": {
    "ipaddr.js": "^1.9.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
